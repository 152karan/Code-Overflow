// java program to calculate length of a String
import java.util.*; 

public class CodeOverFlow{ 

	// Method to calculate length
	private int findStrLen(String str) 
	{ 

		if (str.equals("")) 
			return 0; 
		else
			return findStrLen(str.substring(1)) + 1; 
	} 

//Testing above method calling it from main method
	public static void main(String[] args) 
	{ 
		String str ="CodeOverFlow"; 
		System.out.println(findStrLen(str)); 
	} 
} 
