class FindFactorial{  
 public static void main(String args[]){  
  int i,fact=1;  
  int number=7//Here is the number to calculate factorial    
  
  //looping number 
  for(i=1;i<=number;i++){    
      fact=fact*i;    
  }    
  //Here we will get print of Factorial
  System.out.println("Factorial of "+number+" is: "+fact);    
 }  
}  
