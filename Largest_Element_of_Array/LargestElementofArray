public class LargestElement_array {  
    public static void main(String[] args) {  
  
        //Array intialization
        int [] arr = new int [] {22, 12, 100, 20, 56};  
        
        int max = arr[0];  
      
        
        for (int i = 0; i < arr.length; i++) {  
            //Compare elements of array 
           if(arr[i] > max)  
               max = arr[i];  
        }  
        System.out.println("Largest element " + max);  
    }  
}  
