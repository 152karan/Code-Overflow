public class staticblock {
    static double sqrtA;
    static double sqrtB;
    static{
        //System.out.println("System inside static block: ");
        sqrtA=Math.sqrt(2.00);
        sqrtB=Math.sqrt(3.00);
    }
    static void chetan(){
        System.out.println("jai shrree balaji");
    }
    

}
class staticmain{
    public static void main(String args[]){
        staticblock sb=new staticblock();//
        System.out.println(staticblock.sqrtA);
        staticblock.chetan();
    }
}
